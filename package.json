{
  "name": "saamsa",
  "version": "1.0.0",
  "description": "an easy to use Kafka consumer/topic visualizer and topic replicator/load-balancer",
  "main": "./dist/server/server.js",
  "scripts": {
    "webpack": "tsc client/index.tsx --declaration --outDir ./dist --module es2015 --jsx react --moduleResolution node && cp -r ./client/scss ./dist/client/scss && webpack && tsc ./server/server.ts ./server/createServer.ts --moduleResolution node --outDir ./dist/ --esModuleInterop true",
    "start": "tsc ./index.ts --moduleResolution node --outDir ./dist/electron --esModuleInterop true && nodemon ./dist/server/server.js",
    "test": "NODE_ENV=testing jest SignUpPageUnitTests GraphUnitTests SelectorUnitTests LoginPageUnitTests && NODE_ENV=testing jest --env=node serverUnitTests sqlUnitTests",
    "both": "npm run webpack && npm run start",
    "build": "tsc -p ./ && webpack",
    "package": "electron-forge package",
    "make": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/saamsa.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/firstrepo/issues"
  },
  "homepage": "https://github.com/oslabs-beta/firstrepo#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/cookie-parser": "^1.4.2",
    "@types/d3": "^7.0.0",
    "@types/d3-scale": "^4.0.1",
    "@types/electron": "^1.6.10",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/kafkajs": "^1.9.0",
    "@types/mongoose": "^5.11.97",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.2.4",
    "electron": "^15.3.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "kafkajs": "^1.16.0-beta.22",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.5",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.2",
    "@types/d3-brush": "^3.0.1",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/supertest": "^2.0.11",
    "axios": "^0.22.0",
    "babel-loader": "^8.2.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "cookie-parser": "^1.4.5",
    "css-loader": "^6.3.0",
    "d3": "6.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "java-caller": "^2.4.0",
    "jest": "^27.2.5",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.9",
    "nodemon": "^2.0.14",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "sqlite": "^4.0.23",
    "sqlite3": "^4.2.0",
    "style-loader": "^3.3.0",
    "supertest": "^6.1.6",
    "typescript": "^4.4.4",
    "webpack": "^5.56.0",
    "webpack-dev-server": "^4.3.0"
  },
  "build": {
    "directories": {
      "output": "out"
    },
    "asar": "false",
    "files": [
      "build",
      "index.html",
      "dist"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "dir": "./dist/electron"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "firstrepo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
